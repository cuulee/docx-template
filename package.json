{
  "name": "docx-template",
  "description": "customizing docx template with data",
  "version": "0.2.4",
  "author": {
    "name": "Raymond Li",
    "email": "lalalic@139.com"
  },
  "keywords": [
    "docx",
    "template",
    "docx4js",
    "personalize",
    "customize"
  ],
  "license": "GPL",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/lalalic/docx-template.git"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "browserify": "^13.0.0",
    "jasmine": "^2.2.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "uglify-js": "^2.6.2",
	"uglifyify": "^3.0.1",
    "watchify": ">=3.0.0 <4.0.0"
  },
  "dependencies": {
    "escodegen": "^1.8.0",
    "esprima": "^2.7.2",
    "jszip": "^2.6.0",
    "xmldom": "^0.1.19",
    "docx4js": ">=1.3.2",
    "request": "^2.67.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "babel-plugin-add-module-exports",
      "babel-plugin-transform-object-rest-spread",
	  "babel-plugin-transform-class-properties"
    ]
  },
  "main": "lib/",
  "scripts": {
    "build": "babel src -d lib --source-maps inline",
    "dist": "watchify -v -d -r ./lib:docx-template -o dist/index.js  -i jquery",
    "dist.min": "browserify -g uglifyify -r ./lib:docx-template -o dist/docx-template.js -i jquery",
	
    "test": "jasmine",
    "test.debug": "node --debug-brk .jasmine.js",
    "karma": "karma start"
  }
}
